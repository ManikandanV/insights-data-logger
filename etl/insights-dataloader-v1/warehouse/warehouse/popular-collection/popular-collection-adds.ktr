    <connection>gooru_prod1</connection>
    <sql>select  
date_format(si.added_time,&apos;%Y%m%d&apos;) time_code_mi 
, c.gooru_oid 
, si.content_id
, si.added_time
, count(1) as total_adds
, 0 as total_time_millisecs
, 2 as resource_type
from shelf_item si 
inner join content c on c.content_id = si.content_id 
group by 1, c.gooru_oid</sql>
