  <step>
    <name>Get question attempts from production</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>prod_readonly</connection>
    <sql>SELECT 
date_format(qai.presented_at_time,&apos;%Y%m%d&apos;) time_code, 
c.gooru_oid,
c.creator_id as user_id,
qai.question_id as resource_id,
qa.assessment_id AS parent_id,
SUM(TIME_TO_SEC (TIMEDIFF(qai.answered_at_time, qai.presented_at_time ))) as total_time_millisecs, 
sum(case when qat.attempt_try_status = 1 then 1 else 0 end) as total_score,
count(qai.attempt_id ) as total_attempts,
5 as resource_type 
FROM assessment_attempt_item qai  
INNER JOIN assessment_attempt_try qat ON (qat.attempt_item_id = qai.attempt_item_id)  
INNER JOIN assessment_attempt qa ON (qa.attempt_id = qai.attempt_id)
INNER JOIN content c ON (c.content_id = qai.question_id)
group by 1, 2;</sql>
