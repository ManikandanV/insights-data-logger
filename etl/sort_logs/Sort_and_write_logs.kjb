    <entry>
      <name>Safe to continue?</name>
      <description>Shell</description>
      <type>SHELL</type>
      <filename/>
      <work_directory>${Internal.Job.Filename.Directory}</work_directory>
      <arg_from_previous>N</arg_from_previous>
      <exec_per_row>N</exec_per_row>
      <set_logfile>N</set_logfile>
      <logfile/>
      <set_append_logfile>N</set_append_logfile>
      <logext/>
      <add_date>N</add_date>
      <add_time>N</add_time>
      <insertScript>Y</insertScript>
      <script>set -e 

lockfile=job.lock
pidfile=job.pid
retval=1

if ( set -o noclobber; echo &quot;$PPID&quot; &gt; &quot;$lockfile&quot;) 2&gt; &#47;dev&#47;null; 
then

   trap &apos;rm -f &quot;$lockfile&quot;; exit $?&apos; INT TERM EXIT

   if [ ! -e $pidfile ]; then
      echo &quot;pid file does not exist, creating for kettle PID $PPID&quot;
      echo &quot;$PPID&quot; &gt; &quot;$pidfile&quot;  
      retval=0
   else
	savedpid=`cat &quot;$pidfile&quot;`
	echo &quot;pid file exists, checking if process $savedpid is a running kettle process&quot;
	valid=`ps -aef | grep $savedpid | grep &apos;pentaho&apos; | wc -l | sed -e &apos;s&#47;\s&#47;&#47;g&apos;`
		
	if [ 1 -eq $valid ]; then
		echo &quot;kettle process $savedpid found, exiting&quot;
		retval=1
	else
		echo &quot;stale kettle process $savedpid not found, will continue&quot;
		echo &quot;$PPID&quot; &gt; &quot;$pidfile&quot;  
      		retval=0
	fi
   fi
   rm -f &quot;$lockfile&quot;
   trap - INT TERM EXIT
else
   echo &quot;Failed to acquire lockfile: $lockfile.&quot; 
   echo &quot;Held by $(cat $lockfile)&quot;
   retval=1
fi 

exit $retval</script>
